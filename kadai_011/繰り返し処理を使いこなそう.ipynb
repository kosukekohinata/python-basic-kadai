        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "# 外側のループ: 2回繰り返す\n",
        "count = 0\n",
        "while count < 2:\n",
        "    # 内側のループ: リストの要素を順番に出力\n",
        "    for i in array:\n",
        "        print(i)\n",
        "    count += 1 # 繰り返し回数を増やす"
